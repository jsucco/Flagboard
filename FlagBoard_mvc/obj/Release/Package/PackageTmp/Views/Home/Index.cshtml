        getFieldValue: function(FieldName, DataSourceRow) {
            var deferred = new $.Deferred();
            $.each(DataSourceRow, function (key, value) {
                if (key == FieldName) {
                    deferred.resolve(value);
                    return deferred.promise();
                }
            });
            deferred.resolve(null);
            return deferred.promise();
